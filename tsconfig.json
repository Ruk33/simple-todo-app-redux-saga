{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "checkJs": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "target": "es2015"
    },
    "include": ["src"]
}
